<%- include('../partials/header') %>

<table id="list">
    <thead>
      <tr>
        <th>Airline</th>
        <th>Airport</th>
        <th>Flight<br>No.</th>
        <th>Departure<br>time</th>
        <th></th>
      </tr>
    </thead>
    <tbody>
      <% flights.forEach(function(f) { %>
        <tr>
          <td><%= f.airline %></td>
          <td><%= f.airport %></td>
          <!-- <td><a href="/flights/<%= f._id %>">DETAILS</a></td> -->
          <td><a href="/flights/<%= f._id %>"><%= f.flightNo %></a></td>
          <!-- <td><%= f.departs %></td>        -->
        </tr>
        <% }); %>
      </tbody>
    </table>



<%- include('../partials/footer') %>